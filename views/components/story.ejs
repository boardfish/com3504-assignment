<div class="card mb-3" id="story-<%= story._id %>">
  <div class="card-header d-flex align-items-center">
    <%- include("profile", story.user) %>
    <span class="small text-muted ml-auto"><%= story.createdAt.toLocaleString() %></p>
  </div>
  <div class="card-body">
    <p><%= story.text %></p>
    <div class="photos">
      <% (story.photos || []).forEach(photo => { %>
        <img class="h-100" src="<%= photo %>" />
      <% }) %>
    </div>
  </div>
  <div class="card-footer d-flex align-items-center w-100">
    <%- include("likes",{ ratings: story.likes.reduce((a, v) => { a[v.vote] += 1; return a }, [0, 0, 0, 0, 0])
, html_class: 'flex-grow-1 mr-2' }) %>
    <%- include("likeButton", { storyId: story._id, rating: 0 }) %>
  </div>
</div>